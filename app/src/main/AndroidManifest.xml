<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.app_booking_car">

    <uses-permission
        android:name="android.permission.READ_EXTERNAL_STORAGE"
        android:maxSdkVersion="32" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/ic_car"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_load"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppBookingCar"
        tools:targetApi="31">
        <activity
            android:name=".Detail.SeatDetailActivity"
            android:exported="false"
            android:label="Seat Detail Activity" />
        <activity
            android:name=".Admin.Activity.Fragment.UserFragment"
            android:exported="false" />
        <activity
            android:name=".Admin.Dialog.DialogCar"
            android:exported="false" />
        <activity
            android:name=".Admin.Activity.AdminHomeActivity"
            android:exported="false" />
        <activity
            android:name=".Login.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Login.ForgotPasswordActivity"
            android:exported="false" />
        <activity
            android:name=".User.Activity.HomeActivity"
            android:exported="false" />
        <activity
            android:name=".Login.RegisterActivity"
            android:exported="false" />
    </application>

</manifest>